void Solve() {
	string s;
	cin>>s;
	for(int i=0;i<s.length();i++){
		if(s[i]=='.'){
        cout<<0;
        continue;
    }
    if(s[i]=='-'&&s[i+1]=='.'){
        cout<<1;
        i++;
        continue;
    }
    if(s[i]=='-'&&s[i+1]=='-'){
         cout<<2;
         i++;
    }
	}
	// for(int i=0;i<s.length();i++){
	// 	cout<<s[i];
	// }
}
